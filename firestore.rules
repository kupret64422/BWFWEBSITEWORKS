rules_version = '2';
service cloud.firestore {
match /databases/{database}/documents {

match /artifacts/{appId}/public/data/test_messages/{messageId} {
  
  /* Allow any authenticated user to read all public messages */
  allow read: if true;

  /* Allow authenticated users to create new messages */
  allow write: if request.auth != null;
}

/* Deny all other access by default to everything else in the database */
match /{document=**} {
  allow read, write: if false;
}


}
}
